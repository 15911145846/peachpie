{
  "version": "0.5.0-*",

  "packOptions": {
    "tags": [
      "php",
      "peachpie",
      "dotnet",
      "compiler"
    ],
    "summary": "Peachpie code analysis and compiler core.",
    "owners": [ "jmisek", "iolevel", "peachpie" ],
    "projectUrl": "https://github.com/iolevel/peachpie"
  },

  "dependencies": {
    "Devsense.Php.Parser": "1.1.20",
    "Peachpie.Microsoft.CodeAnalysis": "0.5.2",
    "System.Collections.Immutable": "1.3.1",
    "System.Reflection.Metadata": "1.4.2",
    "Peachpie.Runtime": { "target": "project" }
  },
  "frameworks": {
    "net46": {
    },
    "netstandard1.6": {
      "imports": [ "dnxcore50" ],
      "dependencies": {
        "System.Threading.Tasks.Parallel": "4.3.0"
      }
    }
  },

  "buildOptions": {
    "publicSign": true,
    "keyFile": "../../build/StrongKeys/core.snk",
    "xmlDoc": true,
    "nowarn": [ "1591" ]
  }
}